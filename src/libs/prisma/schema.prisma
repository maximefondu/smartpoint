generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Route {
  id       Int        @id @default(autoincrement())
  name     String     @default("")
  url      String?
  routeKey RouteKey[]
}

model RouteKey {
  id       Int     @id @default(autoincrement())
  routeId  Int
  key      String  @default("")
  value    String?
  required Boolean @default(false)
  route    Route   @relation(fields: [routeId], references: [id])
}
